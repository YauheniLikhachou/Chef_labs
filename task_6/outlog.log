  else
    converge_by("Create #{@new_resource}") do
      create_printer
    end
  end
end

action :delete do
  if @current_resource.exists
    converge_by("Delete #{@new_resource}") do
      delete_printer
    end
  else
    Chef::Log.info "#{@current_resource} doesn't exist - can't delete."
  end
