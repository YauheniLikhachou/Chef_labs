env DAEMON=<%= node['nginx']['binary'] %>
env PID=<%= node['nginx']['pid'] %>
env CONFIG=<%= node['nginx']['source']['conf_path'] %>

respawn
<% if node['nginx']['upstart']['respawn_limit'] %>
