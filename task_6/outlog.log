<% end %>

pre-start script
  ${DAEMON} -t
  if [ $? -ne 0 ]; then
    exit $?
  fi
end script

<% unless node['nginx']['upstart']['foreground'] %>
expect fork
<% else %>
console output
<% end %>

exec ${DAEMON} -c "${CONFIG}"

